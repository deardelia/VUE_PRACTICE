{
  "name": "vue-d3-barchart",
  "version": "0.0.24",
  "description": "Vue component to draw bar charts using d3 ",
  "main": "dist/vue-d3-barchart.umd.js",
  "scripts": {
    "dev": "vue-cli-service serve",
    "example": "vue-cli-service build --dest example",
    "build": "npm run build-component && npm run clean-dist",
    "build-component": "vue-cli-service build --target lib --name vue-d3-barchart src/vue-d3-barchart.vue",
    "lint": "vue-cli-service lint",
    "clean-dist": "rm dist/demo.html"
  },
  "author": "emi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/emiliorizzo/vue-d3-barchart"
  },
  "bugs": {
    "url": "https://github.com/emiliorizzo/vue-d3-barchart/issues"
  },
  "keywords": [
    "vue",
    "vuejs",
    "d3",
    "network",
    "d3-scale",
    "graph",
    "component"
  ],
  "dependencies": {
    "d3-array": "^1.2.0",
    "d3-scale": "^1.0.6",
    "d3-shape": "^1.2.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-rc.8",
    "@vue/cli-plugin-eslint": "^3.0.0-rc.8",
    "@vue/cli-service": "^3.0.0-rc.8",
    "@vue/eslint-config-standard": "^3.0.0-rc.8",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "raw-loader": "^0.5.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue": "^2.5.16",
    "vue-template-compiler": "^2.5.16"
  }
}
